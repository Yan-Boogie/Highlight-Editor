{
  "name": "skyeye-highlight-editor",
  "private": "true",
  "workspaces": [
    "packages/*"
  ],
  "version": "1.0.0",
  "author": "Ian Lai <ian24257587@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build:rollup": "rollup --config ./rollup.config.js",
    "build:rollup:watch": "yarn build:rollup --watch",
    "commit": "git-cz",
    "lint:ts": "eslint . --config .eslintrc --ext .js,.ts",
    "open": "open http://localhost:3001",
    "serve": "cd ./site && next -p 3001",
    "start": "yarn serve",
    "highlight-editor:test": "yarn workspace skyeye-highlight-editor test",
    "highlight-editor:test:watch": "yarn workspace skyeye-highlight-editor test:watch",
    "highlight-editor:test:coverage": "yarn highlight-editor:test --coverage"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@emotion/css": "^11.9.0",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "4",
    "lerna": "^4.0.0",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.4",
    "next": "^10.2.3",
    "prettier": "^2.2.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rollup": "^2.26.6",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-typescript2": "^0.27.2",
    "slate": "^0.78.0",
    "slate-history": "^0.66.0",
    "slate-react": "^0.77.4",
    "source-map-loader": "^0.2.4",
    "skyeye-highlight-editor": "1.0.0",
    "typescript": "^4.2.3"
  }
}
